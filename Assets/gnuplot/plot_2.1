set terminal png  enhanced 
set output "final.png"

# Set ranges and labels for axes
set xrange [0:1.0]
set yrange [0:1.0]
set xlabel "x"
set ylabel "y"

# Enforce an aspect ratio of 1
set size square

# Set the range of the colour scale
set cbrange [0:1]

# Plot the data
plot "final.dat" with image 

# End of file
